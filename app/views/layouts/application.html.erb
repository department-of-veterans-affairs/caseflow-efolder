  <%= favicon_link_tag %>

</head>
<body>
  <div class="content">
    <header>
      <nav class="cf-nav">
        <div class="usa-grid-full">
          <a class="cf-header-link" href="/">
            <h1 class="cf-logo"><span class="cf-logo-image"></span>eFolder Express</h1>
          </a>
          <h2 id="page-title" class="cf-application-title"><%= yield :page_title %></h2>

          <% if !current_user.nil? %>
            <div class="cf-dropdown cf-nav-dropdown">
              <a href="#menu" class="cf-dropdown-trigger" id="menu-trigger">
                  <%= current_user.display_name %>
              </a>

              <ul id="menu" class="cf-dropdown-menu" aria-labelledby="menu-trigger">
                <li><a href="<%= url_for controller: 'help', action: 'show'%>">Help</a></li>
                <li><a href="<%= feedback_url %>" target="_blank">Send Feedback</a></li>
                <li>
                  <div class="dropdown-border"></div>
                  <a href="<%= url_for controller: 'sessions', action: 'destroy'%>">Sign out</a>
                </li>
              </ul>
            </div>
          <% end %>
        </div>
      </nav>
    </header>

    <main class="usa-grid">
      <div class="cf-app">
        <%= yield %>
      </div>
    </main>

  </div>
  <footer class="cf-txt-c usa-grid cf-app-footer">
    <div>
      <div class="cf-push-left">
        <span <% if Rails.application.config.build_version %>title="<%= Rails.application.config.build_version[:date] %>"<% end %>>
         Built
        </span>
        with <abbr title="love">&#9825;</abbr> by the
        <a href="http://www.va.gov/ds/">Digital Service at the <abbr title="Department of Veterans Affairs">VA</abbr></a>
      </div>
      <div class="cf-push-right">
        <a target="_blank" href="<%= feedback_url %>">
          Send feedback
        </a>
      </div>
    </div>
  </footer>
</body>
</html>
<% if Rails.application.config.build_version %>
